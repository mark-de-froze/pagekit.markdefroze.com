<% content_for :links do %>
    <%= link_to t(:back), users_keys_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @users_key, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @users_key, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', users_key: @users_key %>

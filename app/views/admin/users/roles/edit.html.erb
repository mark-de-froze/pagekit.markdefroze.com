<% content_for :links do %>
    <%= link_to t(:back), users_roles_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @users_role, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @users_role, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>

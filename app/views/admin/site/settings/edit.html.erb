<% content_for :links do %>
    <%= link_to t(:back), site_settings_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @site_setting, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @site_setting, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', site_setting: @site_setting %>

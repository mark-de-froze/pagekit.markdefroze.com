<% content_for :links do %>
    <%= link_to t(:back), site_pages_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @site_page, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @site_page, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', site_page: @site_page %>

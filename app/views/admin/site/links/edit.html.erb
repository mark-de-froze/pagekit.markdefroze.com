<% content_for :links do %>
    <%= link_to t(:back), site_links_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @site_link, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @site_link, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', site_link: @site_link %>

<% content_for :links do %>
    <%= link_to t(:back), portfolio_feedbacks_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @portfolio_feedback, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @portfolio_feedback, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', portfolio_feedback: @portfolio_feedback %>

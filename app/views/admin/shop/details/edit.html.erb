<% content_for :links do %>
    <%= link_to t(:back), shop_details_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @shop_detail, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @shop_detail, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', shop_detail: @shop_detail %>

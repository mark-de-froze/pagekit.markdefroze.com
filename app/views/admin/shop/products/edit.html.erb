<% content_for :links do %>
    <%= link_to t(:back), shop_products_path, class: 'uk-button uk-button-default' %>
    <%= link_to t(:show), @shop_product, class: 'uk-button uk-button-default' %>
    <%= link_to t(:destroy), @shop_product, method: :delete, class: 'uk-button uk-button-danger', data: { confirm: t(:confirm) } %>
<% end %>

<%= render 'form', shop_product: @shop_product %>

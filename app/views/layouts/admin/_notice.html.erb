<% if notice %>
<div class="uk-alert-primary" uk-alert>
  <a class="uk-alert-close" uk-close></a>
  <p><%= notice %></p>
</div>
<% end %>

<% if alert %>
    <div class="uk-alert-danger" uk-alert>
      <a class="uk-alert-close" uk-close></a>
      <p><%= alert %></p>
    </div>
<% end %>